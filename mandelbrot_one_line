mandelbrot=lambda:([w:=50,h:=50,max_iter:=100,r:=(-0.7,0.7),i:=(0,1)],"\n".join(["".join([[".,*3456789abcde."[int(([n:=0,z:=0]+[[z:=z*z+complex(r[0]+(x/w)*(r[1]-r[0]),i[0]+(y/h)*(i[1]-i[0])),n:=n+1]for _ in range(max_iter)if abs(z)<=2])[-1][1]*15.8/max_iter)]][-1]*2 for x in range(-w,w)])for y in range(-h,h)]))[-1]
